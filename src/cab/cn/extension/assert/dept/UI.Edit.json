{
    "_form": {
        "window": 0.15,
        "ui": [
            [
                "title=组基本信息"
            ],
            [
                "name,用户组名称,14"
            ],
            [
                "code,用户组编码,14"
            ],
            [
                "parentId,父组,14"
            ],
            [
                "title=业务模型信息"
            ],
            [
                "modelId,描述模型,14"
            ],
            [
                "modelKey,描述记录,14"
            ],
            [
                {
                    "metadata": "$button",
                    "hidden": true,
                    "optionJsx.extension": [
                        "$opSave,保存,$opSave,primary",
                        "$opDelete,删除,$opDelete",
                        "$opReset,重置,$opReset"
                    ],
                    "span": 24
                }
            ]
        ],
        "hidden": {
            "inputs": [
                "key"
            ]
        },
        "actions": {
            "op": {
                "$opSave": "act.group.save",
                "$opDelete": "act.group.delete"
            }
        }
    },
    "_modal": {
        "success": {
            "saved": "您好，您的用户组信息更新成功！",
            "removed": "当前用户组信息已删除成功！"
        }
    }
}