{
    "_page": {
        "title": "ComplexList例子"
    },
    "_grid": {
        "query": {
            "projection": [],
            "pager": "1,8",
            "sorter": "updateTime=DESC",
            "criteria": {}
        },
        "options": {
            "mock.enabled": true,
            "mock.keys": "code,name,description,manager,active",
            "search.cond.message": {
                "code": "代码",
                "name": "名称",
                "manager": "经理",
                "active": "启用"
            },
            "search.cond": [
                "name,c",
                "code,c"
            ],
            "search.enabled": true,
            "search.placeholder": "名称/代码",
            "search.advanced": true,
            "search.advanced.width": "30%",
            "search.advanced.title": "搜索部门",
            "tabs.list": "部门列表",
            "tabs.add": "添加部门",
            "tabs.edit": "编辑部门",
            "op.add": "添加",
            "op.batch.edit": "批量更新",
            "op.batch.delete": "批量删除",
            "op.extra.column": "修改显示列",
            "op.extra.export": "导出",
            "op.extra.import": "导入",
            "window.batch.edit": "选择批量更新的字段,更新,关闭,false,640,true,btnBatchEdit",
            "window.extra.column": "请选择您要保存的列,保存,关闭,false,640,true,btnColumn",
            "window.extra.export": "选择导出字段,导出,关闭,false,720,true,btnExport",
            "window.extra.import": "请上传导入文件,导入,关闭,false,720,true,btnImport",
            "column.dynamic": true,
            "submit.add": "$opAdd",
            "submit.edit": "$opSave",
            "submit.reset": "$opReset",
            "ajax.get.uri": "/api/dept/:id",
            "ajax.delete.uri": "/api/dept/:id",
            "ajax.batch.delete.uri": "/api/depts",
            "ajax.batch.put.uri": "/api/depts",
            "confirm.delete": "确认删除当前部门记录？",
            "confirm.batch.delete": "您确认要删除所有选中的部门记录？"
        },
        "component": {
            "extra.export": {
                "message": "请选择字段！",
                "description": [
                    "只有您有权限的字段会列举在列表中，如果您对该字段没有权限，请联系管理员。",
                    "如果您没有找到您想要的字段，请联系管理员。"
                ]
            },
            "extra.import": {
                "message": "选择本地文件",
                "description": [
                    "您可以按照提供的模板导入Excel格式的数据。",
                    "如果有相关疑问可联系管理员！"
                ],
                "upload": "模板文件"
            },
            "extra.column": {
            },
            "batch.editor": {
                "fieldColumn": {
                    "title": "选择属性",
                    "items": [
                        "code,部门编号",
                        "name,部门名称",
                        "manager,项目经理",
                        "active,是否启用",
                        "created_date,建立日期"
                    ]
                },
                "valueColumn": {
                    "title": "设置值",
                    "render": {
                        "code": "TEXT",
                        "name": "TEXT",
                        "manager": "TEXT",
                        "active": {
                            "type": "RADIO",
                            "config": [
                                "true,启用",
                                "false,禁用"
                            ]
                        },
                        "created_date": {
                            "type": "DATE",
                            "config": {
                                "format": "YYYY-MM-DD"
                            }
                        }
                    }
                },
                "button": "btnBatchEdit",
                "modal": {
                    "error": {
                        "content": "对不起，请选择您要批量更新的属性！"
                    },
                    "success": {
                        "content": "您的属性信息已经全部更新成功！"
                    }
                }
            }
        },
        "table": {
            "total": {
                "report": "（总共:page页，:total条数据）",
                "selected": "勾选了:count条"
            },
            "row": {
                "onDoubleClick": "rxEdit"
            },
            "columns": [
                {
                    "title": "操作",
                    "dataIndex": "key",
                    "fixed": "left",
                    "$render": "ACTION",
                    "$option": [
                        {
                            "text": "编辑",
                            "executor": "rxEdit"
                        },
                        "divider",
                        {
                            "text": "删除",
                            "executor": "rxDelete",
                            "confirm": "确认删除选择的部门记录？"
                        }
                    ]
                },
                {
                    "title": "部门编号",
                    "dataIndex": "code",
                    "sorter": true,
                    "$filter.type": "SEARCH",
                    "$filter.config": {
                        "placeholder": "输入部门编码",
                        "button": {
                            "search": "搜索",
                            "reset": "重置"
                        }
                    }
                },
                {
                    "title": "部门名称",
                    "dataIndex": "name",
                    "sorter": true
                },
                {
                    "title": "是否启用",
                    "dataIndex": "active",
                    "sorter": true,
                    "$render": "LOGICAL",
                    "$mapping": {
                        "true": "启用",
                        "false": "禁用"
                    },
                    "$filter.config.items": [
                        "true,启用",
                        "false,禁用"
                    ],
                    "$filter.config.button": {
                        "yes": "确认",
                        "reset": "重置"
                    },
                    "$filter.config.width": {
                        "radio": 110,
                        "button": 55
                    }
                },
                {
                    "title": "创建时间",
                    "dataIndex": "created_date",
                    "$render": "DATE",
                    "$format": "YYYY-MM-DD"
                }
            ]
        }
    }
}